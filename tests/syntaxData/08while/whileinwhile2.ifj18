# Program 3: Prace s retezci a vestavenymi funkcemi

s1 = 0
s2 = 0
s3 = 0
s4 = 0
print "Vnoreny while 2", s1, "\n"

while s1 != 3 do
	s1 = s1 + 1
	print "s1=", s1, "\n"
	while s2 != 3 do
		s2 = s2 + 1
		print "s2=", s2, "\n"
		while s3 != 3 do
			s3 = s3 + 1
			print "s3=", s3, "\n"
			while s4 != 3 do
				s4 = s4 + 1
				print "s4=", s4, "\n"
			end
		end
	end
end
